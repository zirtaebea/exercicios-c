#include<stdio.h>
#include<locale.h>

int main (){
	
	setlocale(LC_ALL,"portuguese");
	
	int num[4][4];
	int maior=0,contador=0;
	
	for(int i=0;i<4;i++){
		for(int j=0; j<4; j++){
		printf("Digite um valor: ");
		scanf("%d", &num[i][j]);
		
	}
}

	for(int i=0;i<4;i++){	
		for(int j=0; j<4; j++){
			printf("\nNum [%d][%d] vale %d", i, j, num[i][j]);
	}
}
		
		
		for(int i=0;i<4;i++){	
		for(int j=0; j<4; j++){
			contador=num[i][j];
			if (contador>10){
			maior=maior+1;
			}
	}
}

		printf("\n\n%d elementos s√£o maiores que 10", maior);
		
		return 0;
	
}
